<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <description>Vykazovani casu</description>
 <display-name>Vykazovani casu</display-name>
 <context-param>
  <param-name>db-db</param-name>
  <param-value>pokus</param-value>
 </context-param>
 <context-param>
  <param-name>db-machine</param-name>
  <param-value>localhost:3306</param-value>
 </context-param>
 <context-param>
  <param-name>db-pass</param-name>
  <param-value/>
 </context-param>
 <context-param>
  <param-name>db-user</param-name>
  <param-value>root</param-value>
 </context-param>
 <filter>
  <display-name>EncodingFilter</display-name>
  <filter-name>EncodingFilter</filter-name>
  <filter-class>servlety.filtery.EncodingFilter</filter-class>
  <init-param>
   <param-name>encoding</param-name>
   <param-value>UTF-8</param-value>
  </init-param>
 </filter>
 <filter-mapping>
  <filter-name>EncodingFilter</filter-name>
  <url-pattern>*</url-pattern>
 </filter-mapping>
 <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>servlety.role.LoginZcu</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>log</servlet-name>
  <servlet-class>servlety.role.LoginMimo</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>uvodni</servlet-name>
  <servlet-class>servlety.role.Uvod</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>nastaveni</servlet-name>
  <servlet-class>servlety.role.zamestnanec.Nastaveni</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>prehled</servlet-name>
  <servlet-class>servlety.role.zamestnanec.Prehled</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>import</servlet-name>
  <servlet-class>servlety.role.zamestnanec.Import</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>export</servlet-name>
  <servlet-class>servlety.role.zamestnanec.Export</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>vytvoreni</servlet-name>
  <servlet-class>servlety.role.zamestnanec.Vytvoreni</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>registrace</servlet-name>
  <servlet-class>servlety.role.zamestnanec.Registrace</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>log</servlet-name>
  <url-pattern>/log</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>registrace</servlet-name>
  <url-pattern>/registrace</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>uvodni</servlet-name>
  <url-pattern>/uvodni</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>nastaveni</servlet-name>
  <url-pattern>/nastaveni</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>prehled</servlet-name>
  <url-pattern>/prehled</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>import</servlet-name>
  <url-pattern>/import</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>export</servlet-name>
  <url-pattern>/export</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>vytvoreni</servlet-name>
  <url-pattern>/vytvoreni</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>uvodni</welcome-file>
 </welcome-file-list>
</web-app>
